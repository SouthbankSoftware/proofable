{
  "name": "proofable",
  "version": "0.0.1",
  "description": "Proofable Node SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "example": "npm run build && node dist/examples/typescript.js",
    "example-js": "npm run build && node dist/examples/javascript.js",
    "build": "tsc -p tsconfig.build.json && cpx 'src/**/*.{js,d.ts}' dist",
    "clean": "rimraf ./dist '*.proofable' '*.subproofable' '*.dot'",
    "doc": "typedoc && touch docs/.nojekyll",
    "lint": "eslint . --ext .ts,.js --max-warnings=0",
    "depcheck": "depcheck",
    "jest": "jest --forceExit",
    "test": "npm run depcheck && npm run lint && npm run jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint -- --fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SouthbankSoftware/proofable/tree/master/node_sdk"
  },
  "keywords": [
    "proofable",
    "provendb",
    "anchor",
    "blockchain",
    "proof",
    "bitcoin",
    "ethereum",
    "hedera",
    "trie",
    "key",
    "values",
    "digital",
    "assets"
  ],
  "contributors": [
    {
      "name": "Guan Gui",
      "email": "guan@southbanksoftware.com"
    },
    {
      "name": "Koustubh Gaikwad",
      "email": "koustubh@southbanksoftware.com"
    }
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "eth-lib": "^0.1.29",
    "event-iterator": "^2.0.0",
    "google-protobuf": "^3.12.2",
    "grpc": "npm:@grpc/grpc-js@^1.0.4",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@deboxsoft/cpx": "^1.5.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/jest": "^26.0.2",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "depcheck": "^0.9.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "grpc-tools": "^1.9.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-protoc-gen": "^0.12.1-pre.3266132",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  }
}
