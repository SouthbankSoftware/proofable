{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": []
        },
        {
            "name": "proofable-cli remote",
            "type": "go",
            "request": "launch",
            "mode": "remote",
            "remotePath": "${workspaceFolder}/cmd/proofable-cli",
            "program": "${workspaceFolder}/cmd/proofable-cli",
            "cwd": "${workspaceFolder}",
            "port": 2345,
            "host": "127.0.0.1",
        },
        {
            "name": "proofable-cli",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/proofable-cli",
            "env": {},
            "args": [
                "auth",
            ],
            "cwd": "${workspaceFolder}",
        },
        {
            "name": "Node SDK",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_sdk/src/index.ts",
            "preLaunchTask": "build node_sdk",
            "outFiles": [
                "${workspaceFolder}/node_sdk/dist/index.js"
            ],
            "cwd": "${workspaceFolder}/node_sdk",
        }
    ]
}
